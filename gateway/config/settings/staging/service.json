{
  "port": 8080,
  "environment": "STAGING",
  "default_host": "http://localhost:8080",
  "timeout": "3s",
  "cache_ttl": "3s",
  "output_encoding": "no-op",
  "extra_config": {
    "router": {
      "return_error_msg": true
    },
    "telemetry/logging": {
      "level": "INFO",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true
    }
  }
}
